{
  "name": "pluspool-visualization-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@zeit/next-css": "^0.2.1-canary.4",
    "datagarrison": "^0.1.0",
    "http": "0.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^8.1.0",
    "next-transpile-modules": "^2.3.1",
    "paper-jsdom-canvas": "^0.12.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "recharts": "^1.6.2",
    "unfetch": "^4.1.0",
    "url": "^0.11.0",
    "webpack": "^4.33.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "eslint-config-xo-react": "^0.19.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "start": "NODE_ENV=production node server.js",
    "lint": "xo"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": "xo-react",
    "parser": "babel-eslint",
    "envs": [
      "browser",
      "node",
      "jest"
    ],
    "rules": {
      "unicorn/filename-case": false,
      "react/default-props-match-prop-types": false
    }
  },
  "engines" : {
    "node" : "~v10.8.0"
  }
}
